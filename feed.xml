---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:content="http://purl.org/rss/1.0/modules/content/">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% for post in site.posts limit:10 %}
      <item>
        <title>{{ post.title | xml_escape }}</title>
        <author>{{ site.default_author | xml_escape }}</author>
        <description>{{ post.content | xml_escape }}</description>
        <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
        <link>{{ post.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ post.url | prepend: site.baseurl | prepend: site.url }}</guid>
        <content:encoded>
          <![CDATA[{{ post.content }}]]>
        </content:encoded>
        {% for tag in post.tags %}
        <category>{{ tag | xml_escape }}</category>
        {% endfor %}
        {% for cat in post.categories %}
        <category>{{ cat | xml_escape }}</category>
        {% endfor %}
      </item>
    {% endfor %}

    {% for i in (1..10) %}
      <item>
        <title>sample article {{ i }}</title>
        <author>{{ site.default_author | xml_escape }}</author>
        <description>description {{ i }}</description>
        <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
        <link>{{ "/" | append:i | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ "/" | append:i | prepend: site.baseurl | prepend: site.url }}</guid>
        <content:encoded>
          <![CDATA[
          <!doctype html>
          <html lang="en" prefix="op: http://media.facebook.com/op#">
          <head>
            <meta charset="utf-8">
            <link rel="canonical" href="{{ "/" | append:i | prepend: site.baseurl | prepend: site.url }}">
            <meta property="op:markup_version" content="v1.0">
          </head>
          <body>
            <article>
              <header>
                <h1>article header h1: test article {{i}}</h1>
                <figure>
                  <img src="{{ site.url }}/assets/rss/img1.jpg" />
                  <figcaption>article header figure: This ramen is amazing</figcaption>
                </figure>
              </header>

              <h1>article h1: test article {{i}}<h1>
              <p>this is test article {{i}}</p>

              <figure>
                <img src="{{ site.url }}/assets/rss/img2.jpg" />
                <figcaption>article figure: This ramen is also amazing</figcaption>
              </figure>

              <footer>this is footer</footer>
            </article>
          </body>
          </html>
          ]]>
        </content:encoded>
        <category>random</category>
      </item>
    {% endfor %}
  </channel>
</rss>
